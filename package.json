    "framer-motion": "^10.16.12",
    "i18next": "^23.7.7",
    "i18next-browser-languagedetector": "^7.2.0",
    "immer": "^10.0.3",
    "query-string": "^8.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.48.2",
    "react-i18next": "^13.5.0",
    "react-icons": "^4.12.0",
    "react-lazily": "^0.9.2",
    "react-number-format": "^5.3.1",
